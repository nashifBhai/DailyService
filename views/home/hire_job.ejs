<!DOCTYPE html>
<html>
<head>
	<title>Hire History List</title>
</head>
<body>
	<h1>Hire History</h1>
	
	<a href="/home">Back</a> | 
	 

	<br>
	<br>

	<table border="1">
		<tr>
			<td>ID</td>
			<td>Name</td>
			<td>WorkHour</td>
			<td>work Status</td>
		</tr>

		<% for(var i=0; i < employeeList.length; i++){ %>

		<tr>
			<td><%= employeeList[i].id %></td>
			<td><%= employeeList[i].name %></td>
			<td><%= employeeList[i].workhour %></td>
			<% if(employeeList[i].work_status) { %>
				<td>part_time</td>
			<% } else { %>
				<td>full_time</td>
			<%  }%>

		</tr>

		<% } %>
	</table>
	<a href="/logout">Logout</a> 

</body>
</html>